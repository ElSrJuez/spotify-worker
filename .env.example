
# Spotify API credentials (do not commit your real secrets)
SPOTIFY_CLIENT_ID=
SPOTIFY_CLIENT_SECRET=
SPOTIFY_REDIRECT_URI=

# Spotify API scope (space-separated list of permissions)
SPOTIFY_SCOPE=playlist-modify-public playlist-modify-private playlist-read-private user-read-private

# Path to store the Spotify OAuth token cache
SPOTIFY_TOKEN_CACHE=.spotify_token_cache
